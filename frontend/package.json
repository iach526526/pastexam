{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.ts,.vue src tests",
    "format": "prettier --write .",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "prepare": "cd .. && husky frontend/.husky"
  },
  "dependencies": {
    "@primeuix/themes": "^1.1.1",
    "@tailwindcss/vite": "^4.1.8",
    "axios": "^1.9.0",
    "dompurify": "^3.3.0",
    "highlight.js": "^11.11.1",
    "marked": "^16.4.1",
    "pdf-lib": "^1.17.1",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.5",
    "tailwindcss": "^4.1.8",
    "vue": "^3.5.13",
    "vue-pdf-embed": "^2.1.3",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@eslint/json": "^0.12.0",
    "@playwright/test": "^1.56.1",
    "@primevue/auto-import-resolver": "^4.3.5",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^4.0.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.28.0",
    "eslint-plugin-vue": "^10.2.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "lint-staged": "^16.1.6",
    "prettier": "3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "unplugin-vue-components": "^28.7.0",
    "vite": "^6.3.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-sitemap": "^0.8.2",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vitest": "^4.0.4"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "npx pnpm exec eslint --ext .js,.ts,.vue src tests",
      "npx pnpm exec prettier --write ."
    ]
  },
  "packageManager": "pnpm@9.0.0"
}
